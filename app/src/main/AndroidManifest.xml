<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.und.sanjaya.database.groceryshopping">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name =".ProductActivity"
            android:label="@string/action_product" />
        <activity android:name =".CouponActivity"
            android:label="@string/action_coupon" />
        <activity android:name =".ShoppingActivity"
            android:label="@string/action_shopping" />
        <activity android:name =".ProductDetailActivity"
            android:label="@string/action_product_detail" />
        <activity android:name =".CouponDetailActivity"
            android:label="@string/action_coupon_detail" />
        <activity android:name =".ProductAddActivity"
            android:label="@string/action_product_add" />
        <activity android:name =".CouponAddActivity"
            android:label="@string/action_coupon_add" />
    </application>

</manifest>